	<script>
		function sprint(){
			document.write(cases.size());
			var i=0;
			for(i=0;i<cases.size();i++){
				
				document.write("id:"+cases[i].case_id);
				document.write("ty:"+cases[i].question_type);
				document.write("time:"+cases[i].question_time);
				document.write("title"+cases[i].question_title);
				document.write("desc:"+cases[i].question_desc);
				document.write("help:"+cases[i].question_help);
			}
		}
	</script>
<title>Insert title here</title>
</head>
<body>
	<script>sprint();</script>
